<html ng-app="appmodule">
<head>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
</head>
<body ng-controller="contactcontroller">
    <h1>Contact Information</h1>

    <h2>
        <span>{{userName}}</span>
        <input type="button" class="btn" value="logoff" ng-click="logout()" />
    </h2>

    <table class="table table-striped table-striped table-bordered">
        <thead>
            <tr>
                <th>ContactID</th>
                <th>ContactName</th>
                <th>ContactNumber</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="Contact in Contacts">
                <td>
                    <span>{{Contact.ContactID}}</span>
                </td>
                <td>
                    <span>{{Contact.ContactName}}</span>
                </td>
                <td>
                    <span>{{Contact.ContactNumber}}</span>
                </td>
            </tr>
        </tbody>
    </table>
    <div><span>{{Message}}</span></div>
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/MyScripts/Module.js"></script>
    <script src="~/Scripts/MyScripts/ContactService.js"></script>
    <script src="~/Scripts/MyScripts/ContactController.js"></script>


</body>
</html>